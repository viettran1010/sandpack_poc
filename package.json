{
  "name": "base_source",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:preview": "nest start --builder webpack",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk --loader tsx node_modules/.bin/jest --runInBand",
    "test:prepare": "NODE_ENV=test yarn db:create:run",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit",
    "typeorm": "tsx ./node_modules/typeorm/cli.js -d src/database/data-source.ts",
    "db:create": "tsx ./node_modules/typeorm-extension/dist/cli/index.js db:create -r src/database -d data-source.ts -f data-source.ts",
    "db:drop": "yarn typeorm schema:drop",
    "db:generate": "yarn typeorm migration:generate src/database/migrations/migrations",
    "db:run": "yarn typeorm migration:run",
    "db:rollback": "yarn typeorm migration:revert",
    "db:create:run": "yarn db:create && yarn db:run",
    "db:seed": "tsx ./node_modules/typeorm-extension/dist/cli/index.js seed -r src/database -d data-source.ts -f data-source.ts",
    "db:init": "yarn db:create && yarn db:generate && yarn db:run",
    "credentials:edit": "node bin/credentials-edit.js",
    "start:repl": "nest start --entryFile repl"
  },
  "dependencies": {
    "@codebrew/nestjs-storage": "0.1.7",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/bull": "0.6.1",
    "@nestjs/cache-manager": "2.0.1",
    "@nestjs/common": "10.0.4",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.4",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.4",
    "@nestjs/swagger": "7.0.12",
    "@slynova/flydrive": "1.0.3",
    "@slynova/flydrive-s3": "1.0.3",
    "bcryptjs": "2.4.3",
    "bull": "4.10.1",
    "cache-manager": "4.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "command-line-args": "5.2.1",
    "dayjs": "1.11.6",
    "dotenv": "16.0.2",
    "express-basic-auth": "1.2.1",
    "hbs": "4.2.0",
    "md5-file": "5.0.0",
    "mime-types": "2.1.35",
    "ms": "2.1.3",
    "nestjs-form-data": "1.8.3",
    "nestjs-i18n": "10.2.6",
    "node-credentials": "1.5.0",
    "nodemailer": "6.8.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "sharp": "0.32.0",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.5",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.4",
    "@nestjs/typeorm": "10.0.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.67",
    "@types/bcryptjs": "2.4.2",
    "@types/bull": "4.10.0",
    "@types/cache-manager": "4.0.2",
    "@types/express": "4.17.14",
    "@types/jest": "28.1.4",
    "@types/lodash": "4.14.187",
    "@types/node": "16.0.0",
    "@types/nodemailer": "6.4.6",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "helmet": "6.0.1",
    "jest": "29.0.3",
    "jest-date-mock": "1.0.8",
    "pg": "8.8.0",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "29.0.1",
    "ts-loader": "9.4.1",
    "tsconfig-paths": "4.1.0",
    "tsconfig-paths-webpack-plugin": "4.0.1",
    "tsx": "3.9.0",
    "typeorm": "0.3.10",
    "typeorm-extension": "2.8.1",
    "typescript": "5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}